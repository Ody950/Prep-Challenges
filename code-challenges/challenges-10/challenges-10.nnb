        {
            "language": "javascript",
            "source": [
                "function migratoryBirds(arr) {\n    \n\n  let maxRepetition= 0;\n  let count = 0;\n    \n      for(let i = 0; i<arr.length; i++){\n        let thisCount = 0;\n        for(let j = -1; j < arr.length; j++){\n            \n          if (arr[i] === arr[j]){\n            thisCount =  thisCount +1; \n            \n          }\n      }\n  \n        if (thisCount > count){\n       maxRepetition = arr[i];\n        count = thisCount;\n        }\n      \n      }\n                \n      \n      \n  \n  return(maxRepetition);\n  }"
