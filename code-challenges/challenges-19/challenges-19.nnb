            "language": "javascript",
            "source": [
                "unction appendAndDelete(s, t, k) {\n  let same = 0;\n  \n  for (let i =0; i<s.length; i++){\n      if(s[i] === t[i]){\n          same ++;\n      }else{\n          break\n      }\n  }\n  \n  let reS = s.length - same\n   let reT = t.length - same\n   \n   if(k >= reS + reT && (k - reS - reT) %2 == 0 || k>= t.length+s.length){\n       return(\"Yes\");\n   }else{\n       return(\"No\");\n   }\n   \n\n}"
            ],
